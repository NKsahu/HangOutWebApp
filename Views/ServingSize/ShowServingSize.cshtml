@model IEnumerable<HangOut.Models.POS.ServingSize>
@{
    Layout = null;
}
<table class="table table-hover w-100" id="TBLID">
    <thead>
        <tr>
            <th>
                Select
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
        </tr>
    </thead>
    <tbody id="Tbody">
        @foreach (var SSize in Model)
        {
            <tr id="@SSize.ServingId">
                <td>
                    @SSize.ServingId
                </td>
                <td>
                    @SSize.Name
                </td>
            </tr>
        }
    </tbody>
</table>

