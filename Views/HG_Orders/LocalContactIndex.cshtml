@model IEnumerable<HangOut.Models.LocalContacts>
    @{
        Layout = null;
        
        }
<div class="card">
    <div class="card-header card-header-color">
        <h3 style="text-align:center;color:white">LOCAL CONTACT</h3>
    </div>
    <div class="card-body">
        <table class="table table-bordered table-hover w-100 "style="width:100%" >
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.MobileNo)
                </th>
                <th> 
                    Customer Name
                </th>
                 
            </tr>

            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MobileNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cust_Name)
                </td>
            </tr>
            }

        </table>
    </div>
</div>
